{
  "name": "@repo/convex",
  "version": "0.1.0",
  "private": true,
  "main": "_generated/api.js",
  "types": "_generated/api.d.ts",
  "exports": {
    "./_generated/api": {
      "types": "./_generated/api.d.ts",
      "default": "./_generated/api.js"
    },
    "./_generated/dataModel": {
      "types": "./_generated/dataModel.d.ts",
      "default": "./_generated/dataModel.js"
    },
    "./types": {
      "types": "./types.ts",
      "default": "./types.ts"
    },
    "./auth": {
      "types": "./auth.ts",
      "default": "./auth.ts"
    }
  },
  "scripts": {
    "dev": "convex dev",
    "build": "tsc --noEmit",
    "deploy": "convex deploy",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@convex-dev/better-auth": "^0.8.6",
    "better-auth": "1.3.8",
    "convex": "^1.27.0",
    "nanoid": "^5.1.6"
